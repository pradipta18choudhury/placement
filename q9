/********
Program to expand/decode a string as follows.
Input: a2b4c3
Output: aabbbbccc

******/

#include<bits/stdc++.h>
using namespace std;
#include<string.h>

int main() {
   char str[50];
    int i=0,j,k,c;
    printf("Enter  String :  ");
    scanf("%s",&str);
    for(i=0;i<strlen(str);) {
        for(j=0;j<str[i+1]-'0';j++) {
            printf("%c",str[i]);
        }
    i=i+2;
    }
}
